{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"jsx": "react",
		"inlineSourceMap": false,
		"rootDir": "src",
		"baseUrl": "./src",
		"paths": {
			"test-utils/*": ["test-utils/*"],
			"Components/*": ["components/*"],
			"Views/*": ["views/*"],
			"Types/*": ["types/*"],
			"Utils/*": ["utils/*"],
			"Providers/*": [".providers/*"]
		}
	},
	"include": ["src"]
}
